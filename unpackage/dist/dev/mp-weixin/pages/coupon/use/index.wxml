<view class="container"><block wx:if="{{myCouponList.length===0}}"><view class="coupon_empty"><u-empty vue-id="18ee58ac-1" text="无可用优惠券" mode="coupon" margin-top="100" bind:__l="__l" vue-slots="{{['bottom']}}"><u-button vue-id="{{('18ee58ac-2')+','+('18ee58ac-1')}}" slot="bottom" size="medium" data-event-opts="{{[['^click',[['handleGoGetCoupon']]]]}}" bind:click="__e" bind:__l="__l" vue-slots="{{['default']}}">点击领取优惠券</u-button></u-empty></view></block><block wx:else><view class="coupon_no_empty"><block wx:for="{{myCouponList}}" wx:for-item="item" wx:for-index="index" wx:key="id"><view class="jingdong"><view class="coupon_checkbox_wrap"><checkbox-group data-id="{{item.id}}" data-event-opts="{{[['change',[['handleCouponSelect',['$event']]]]]}}" bindchange="__e"><checkbox checked="{{item.checked}}"></checkbox></checkbox-group></view><view class="left"><view class="sum">￥<text class="num">{{item.discount}}</text></view><block wx:if="{{item.goodId===0}}"><view class="type">{{"满"+item.couponMin+"元可用"}}</view></block></view><view class="right"><view class="top"><view class="title"><block wx:if="{{item.storeId>0}}"><text class="tag">{{item.storeName}}</text></block><block wx:else><text class="tag">全场通用</text></block><block wx:if="{{item.goodId>0}}"><text class="tag">{{item.goodName}}</text></block><text>{{item.couponName}}</text></view></view><view class="tips"><view class="explain"><text>{{item.couponDesc}}</text></view></view></view></view></block></view></block></view>